{
    "version": "0.2.0",
    "configurations": [
        {
            "presentation": {
                "group": "Start",
                "hidden": false
            },

            "name": "Launch Portrait Filter",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-portraitfilter",
            "program": "${workspaceFolder}/bin/Debug/net8.0/PortraitFilter",
            // "args": [ "--selftest" ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "requireExactSource": false,
            "justMyCode": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "RUNNING_IN_VSCODE": "true",
                "DOTNET_NOLOGO": "true"
            }
        }
    ]
}